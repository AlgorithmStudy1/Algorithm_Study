import sys

N,M=map(int,input().split())
ary=[[] for _ in range(N)]
visited=[0]*N
for i in range(M):
    x,y=map(int,sys.stdin.readline().split())
    ary[x].append(y)
    ary[y].append(x)

def dfs(v,cnt):
    global ans
    visited[v]=1
    if cnt==4:
        ans=True
        return
    for x in ary[v]:
        if visited[x]==0:
            dfs(x,cnt+1)
            visited[x]=0
    
for i in range(N):
    ans=False
    dfs(i,0)
    visited[i]=0
    if ans:
        print(1)
        break
if not ans:
    print(0)
