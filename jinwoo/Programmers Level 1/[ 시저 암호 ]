def solution(s, n):
    answer=''
    for ch in s:
        if ch.isalpha():
            if ord(ch)>=65 and ord(ch)<=90:
                number=ord(ch)+n
                if number>90:
                    number-=26
                answer+=chr(number)
            elif ord(ch)>=97 and ord(ch)<=122:
                number=ord(ch)+n
                if number>122:
                    number-=26
                answer+=chr(number)
        else:
            answer+=" "
    return answer
