from collections import deque

def solution(priorities, location):
    answer=[0 for _ in range(len(priorities))]
    for i in range(len(priorities)):
        priorities[i]=priorities[i],i
    q=deque(priorities)
    num=1
    while q:
        isExist=False
        temp=q.popleft()
        for x in list(q):
            if x[0]>temp[0]:
                q.append(temp)
                isExist=True
                break
        if not isExist:
            answer[temp[1]]=num
            num+=1
    print(answer)
    return answer[location]
