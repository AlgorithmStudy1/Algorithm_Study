def solution(progresses, speeds):
    answer=[]
    days=[]
    for i in range(len(progresses)):
        count=1
        while progresses[i]+speeds[i]*count<100:
            count+=1
        days.append(count)
    print(days)
    count=1
    day=days.pop(0)
    while len(days)!=0:
        if day<days[0]:
            answer.append(count)
            count=1
            day=days.pop(0)
        else:
            count+=1
            days.pop(0)
        
    answer.append(count)
    return answer
